<%= render 'cover' %>

<!-- News -->
<div class="section section--black countdown">
	<div class="container container--no-padding">
		<div class="countdown-headings">
			<div class="countdown-title days">Days</div>
			<div class="countdown-title hours">Hours</div>
			<div class="countdown-title mins">Minutes</div>
			<div class="countdown-title secs">Seconds</div>
		</div>
	</div>
	<div class="container">
		<div class="clock"></div>
	</div>
</div>

<div class="section" id="news">

	<div class="content-wrapper">

		<h2 class="section__header">news</h2>
		
		<div class="container container--half">
			<iframe src="http://www.facebook.com/plugins/likebox.php?href=http%3A%2F%2Fwww.facebook.com%2F763020623758888&amp;width=650&amp;colorscheme=light&amp;show_faces=true&amp;border_color&amp;stream=true&amp;header=true&amp;height=450" scrolling="yes" style="height:450px;  " class="container__iframe" allowtransparency="true" frameborder="0"></iframe>
		</div>

		<div class="container container--half">
			<!-- SnapWidget -->
			<script src="https://snapwidget.com/js/snapwidget.js"></script>
			<iframe src="https://snapwidget.com/embed/241718" class="snapwidget-widget" allowTransparency="true" frameborder="0" scrolling="no" style="border:none; overflow:hidden; width:100%; "></iframe>
		</div>
		<div class="container">
			<h3 class="container__header">checkout last years video</h3>

			<iframe src="http://www.youtube.com/embed/?listType=user_uploads&amp;list=amsterdamconnection" width="480" height="500" class="container__iframe"></iframe>              
		</div>

	</div>
</div>


<!-- Lineup -->


<div class="section section--black" id="lineup">

	<div class="content-wrapper">

		<h2 class="section__header section__header--white">the line-up</h2>
		

		<div class="guest-slider">
